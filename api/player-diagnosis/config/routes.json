{
  "routes": [
    {
      "method": "GET",
      "path": "/player-diagnoses",
      "handler": "player-diagnosis.find",
      "config": {
        "policies": ["global::has-apikey"]
      }
    },
    {
      "method": "GET",
      "path": "/player-diagnoses/count",
      "handler": "player-diagnosis.count",
      "config": {
        "policies": ["global::has-apikey"]
      }
    },
    {
      "method": "GET",
      "path": "/player-diagnoses/:id",
      "handler": "player-diagnosis.findOne",
      "config": {
        "policies": ["global::has-apikey"]
      }
    },
    {
      "method": "POST",
      "path": "/player-diagnoses",
      "handler": "player-diagnosis.create",
      "config": {
        "policies": ["global::has-apikey"]
      }
    },
    {
      "method": "PUT",
      "path": "/player-diagnoses/:id",
      "handler": "player-diagnosis.update",
      "config": {
        "policies": ["global::has-apikey"]
      }
    },
    {
      "method": "DELETE",
      "path": "/player-diagnoses/:id",
      "handler": "player-diagnosis.delete",
      "config": {
        "policies": ["global::has-apikey"]
      }
    }
  ]
}
